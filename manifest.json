{
    "name": "Upwork MCY",
    "description": "Upwork icin bana uygun olan ilanlari alert eder.",
    "version": "1.0",
    "icons": {
      "16": "upwork-16.png",
      "32": "upwork-32.png",
      "48": "upwork-48.png",
      "128": "upwork-128.png"
    },
    "permissions": [
      "tabs",
      "https://www.upwork.com/jobs/*",
      "storage"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "manifest_version": 2,
    "options_page": "options.html",
    "page_action": {
      "default_popup": "popup.html",
      "default_icons": {
        "16": "upwork-16.png",
        "32": "upwork-32.png",
        "48": "upwork-48.png",
        "128": "upwork-128.png"
    }}
    
  }